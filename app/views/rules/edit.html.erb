<h1>Edit Rule</h1>

<%= form_for :rule, url: rule_path(@rule), method: :patch do |f| %>
  <p>
    <%= f.label :param %><br>
    <%= f.text_field :param %>
  </p>
 
  <p>
    <%= f.label :treshold %><br>
    <%= f.text_field :treshold %>
  </p>

  <p>
    <%= f.label :users %><br>
    <%= f.text_field :users %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>

<%= link_to 'Back', rules_path %>